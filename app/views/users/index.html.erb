<h1>Users#index</h1>

<p style="color: green"><%= notice %></p>
<h1>Users</h1>

<div>
    <%@users.each do |user| %>
    <%=user.acc.map(&:name)%>
    <%=render user%>
    <%=link_to 'show this user',user%> | Total User : <%=user.acc.size%> | <%=link_to 'show this users account', user_accounts_path(user)%>
    </br></br>
    <%end%>
    <%=link_to 'New user', new_user_path%>
</div>
