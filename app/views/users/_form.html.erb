<%= form_with(model: user) do |form| %>

<%= yield form %>

<div>
    <%=form.label :name %><br>
    <%=form.text_field :name %>
</div>

<div>
    <%=form.label :age %><br>
    <%=form.text_field :age %>
</div>

<div>
    <%=form.label :role %>
    <%=form.collection_check_boxes :role_ids,Role.all,:id,:name %>
</div>
<div>
    <br><%= form.submit%>
</div>

<%end%>
